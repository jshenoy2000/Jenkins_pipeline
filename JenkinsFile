pipeline{
    agent any
    environment{
        JAG_VERSION="1.35"
    }
    tools{
        maven "maven"
    }
    stages{
        stage("Compile"){
            steps{
                sh 'javac Test.java'
                sh '"compiling version ${JAG_VERSION}"'
            }
        }
        stage("Execute"){
            steps{
                sh 'java Test'
            }
        }
    }
    post{
        always{
            sh 'echo "Always print this"'
        }
        success{
            sh 'echo "Build Success"'
        }
        failure{
            sh 'echo "Failure"'
        }
    }
}